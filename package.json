{
  "name": "foods-with-histamine",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn relay && expo start",
    "android": "yarn start -- --android",
    "ios": "yarn start -- --ios",
    "web": "yarn start -- --web",
    "test": "jest --watchAll",
    "ts:check": "tsc",
    "lint": "eslint --fix .",
